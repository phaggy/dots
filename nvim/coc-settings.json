{
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "codeLens.enable": false,
  "tsserver.formatOnType": false,
  "tsserver.reportStyleChecksAsWarnings": false,
  "tsserver.log": "verbose",
  "javascript.showUnused": false,
  "javascript.validate.enable": false,
  "coc.preferences.formatOnType": false,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "jsx",
    "js",
    "css",
    "liquid"
  ],
  "html.format.indentInnerHtml": true,
  "html.autoClosingTags": true,
  "sh.enable": true,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls-root", "compile_commands.json"],
      "initializationOptions": {
        "cache": {
          "directory": ".ccls-cache",
          "highlight": { "lsRanges": true }
        },
        "client": {
          "snippetSupport": true
        }
      }
    }
  },
  "snippets.extends": {
    "typescript": ["javascript"],
    "typescriptreact": ["javascript"]
  },
  "rust-analyzer.lens.enable": true,
  "rust-analyzer.inlayHints.enable": false,
  "svelte.enable-ts-plugin": true,
  "java.format.settings.url": "/home/hay/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-11",
      "path": "/usr/lib/jvm/java-11-openjdk/bin/java"
    }
  ]
}
